## Nelly Blindenbacher
## July 2023

# Set directory
setwd("\\\\fs-home\\bline$\\Documents\\Projekte\\9 CT Model vs. Neurologist\\2 R Versions\\3 Week 29")

# Load data
clinicaldata_anonym <- read.xlsx("Data_matched_incl_anonymization.xlsx")

# Define the main folder name
main_folder_name <- "Competition DL vs. Neurologist"

# Create the main folder within the current directory
main_folder <- file.path(getwd(), main_folder_name)
if (!dir.exists(main_folder)) {
  dir.create(main_folder)
}

# Create subfolders for each anonym_ID of Part 1
sapply(clinicaldata_anonym$anonym_ID_part1, function(x) {
  dir.create(file.path(main_folder, "Part 1", x), recursive = TRUE)
})

# Create subfolders for each anonym_ID of Part 2
sapply(clinicaldata_anonym$anonym_ID_part2, function(x) {
  dir.create(file.path(main_folder, "Part 2", x), recursive = TRUE)
})
